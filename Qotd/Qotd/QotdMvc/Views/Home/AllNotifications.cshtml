@{
    ViewBag.Title = "All Notifications";
    Layout = ViewBag.Layout;
}
<ul class="breadcrumb">
    <li>
    <a href="@ViewBag.PastPageUrl">@ViewBag.PastPageName</a> <span class="divider">/</span>
    </li>
    <li class="active">@ViewBag.PageFriendlyName</li>
</ul>
@foreach (var notification in ViewBag.Notifications)
{
    <text><div>@Html.Partial("Notification", (Qotd.Entities.Notification)notification)</div></text>
}
@if (ViewBag.More)
{
<div class="ajax-container">
    <div style="overflow: hidden">
        <a href="@Url.Action("AllNotifications", "Home", new { skip = ViewBag.Skip, take = ViewBag.Take, ajax = true })" class="ajax-link"><button class="btn btn-small btn-primary" type="submit">
            <i class="icon-chevron-down"></i>&nbsp;&nbsp;See More</button></a>
    </div>
</div>
}